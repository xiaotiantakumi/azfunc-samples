{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Azure Function",
        "request": "launch",
        "runtimeArgs": [
          "run-script",
          "start"
        ],
        "runtimeVersion": "16",
        "runtimeExecutable": "npm",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "type": "pwa-node",
        "console": "integratedTerminal"
      },
      {
        "name": "Attach to Node Functions",
        "type": "node",
        "request": "attach",
        "port": 9229,
        "preLaunchTask": "func: host start"
      }
    ]
  }